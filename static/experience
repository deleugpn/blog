                ['Database', 'Elasticsearch', new Date(2020, oct, 1), now],

                ['Framework', 'Laravel', new Date(2016, jan, 1), now],
                ['Framework', 'PHPUnit', new Date(2016, apr, 1), now],
                ['Framework', 'PHPStan', new Date(2019, aug, 1), now],
                ['Framework', 'AMFPHP', new Date(2010, oct, 1), new Date(2013, mar, 1)],

                ['Virtualization', 'VirtualBox', new Date(2013, may, 1), new Date(2016, jan, 1)],
                ['Virtualization', 'Vagrant', new Date(2016, jan, 1), new Date(2017, jan, 1)],
                ['Virtualization', 'Docker', new Date(2017, jan, 1), now],
                ['Virtualization', 'Alpine', new Date(2018, jan, 1), now],

                ['Host', 'Plesk', new Date(2010, oct, 1), new Date(2013, nov, 1)],
